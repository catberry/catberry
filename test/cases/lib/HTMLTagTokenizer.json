{
	"cases": [
		{
			"description": "should correctly tokenize empty string",
			"html": "",
			"expected": [
				{
					"name": "END",
					"value": ""
				}
			]
		},
		{
			"description": "should return ILLEGAL on wrong beginning",
			"html": "f",
			"expected": [
				{
					"name": "ILLEGAL",
					"value": "f"
				}
			]
		},
		{
			"description": "should return ILLEGAL on wrong tag name",
			"html": "<>",
			"expected": [
				{
					"name": "TAG_START",
					"value": "<"
				},
				{
					"name": "ILLEGAL",
					"value": ">"
				}
			]
		},
		{
			"description": "should return ILLEGAL on wrong attribute name",
			"html": "<name a\"=123>",
			"expected": [
				{
					"name": "TAG_START",
					"value": "<"
				},
				{
					"name": "TAG_NAME",
					"value": "name"
				},
				{
					"name": "ATTRIBUTE_NAME",
					"value": "a"
				},
				{
					"name": "ILLEGAL",
					"value": "\""
				}
			]
		},
		{
			"description": "should return ILLEGAL if tag ends after equals",
			"html": "<name a=>",
			"expected": [
				{
					"name": "TAG_START",
					"value": "<"
				},
				{
					"name": "TAG_NAME",
					"value": "name"
				},
				{
					"name": "ATTRIBUTE_NAME",
					"value": "a"
				},
				{
					"name": "EQUAL",
					"value": "="
				},
				{
					"name": "ATTRIBUTE_UNQUOTED_VALUE",
					"value": ">"
				},
				{
					"name": "ILLEGAL",
					"value": ""
				}
			]
		},
		{
			"description": "should return ILLEGAL if tag end is absent after tag name",
			"html": "<name",
			"expected": [
				{
					"name": "TAG_START",
					"value": "<"
				},
				{
					"name": "TAG_NAME",
