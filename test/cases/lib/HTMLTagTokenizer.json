				},
				{
					"name": "ILLEGAL",
					"value": ""
				}
			]
		},
		{
			"description": "should return ILLEGAL if wrong tag end",
			"html": "<name /",
			"expected": [
				{
					"name": "TAG_START",
					"value": "<"
				},
				{
					"name": "TAG_NAME",
					"value": "name"
				},
				{
					"name": "ILLEGAL",
					"value": "/"
