{#nickname}
    <form class="message-post-form" name="post" data-module="chat" data-dependents="chat_post">
        Enter message below:<br/>
        <input name="message" type="text"><br/>
        <input type="submit" value="Send">
    </form>
    <a href="#quit">Click here to logout</a>
{:else}
    <a href="#auth">Click here to login</a>
{/nickname}
